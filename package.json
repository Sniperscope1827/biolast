{
  "name": "escape-from-discord",
  "version": "1.0.0",
  "description": "A Discord bot",
  "main": "./build/index.js",
  "author": "blobfysh",
  "engines": {
    "node": ">=14.0.0"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc-watch --noClear -p ./tsconfig.json --onSuccess \"node -r dotenv/config ./build/index.js\"",
    "start": "node -r dotenv/config build/index.js",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "migrate:up": "db-migrate up",
    "migrate:down": "db-migrate down",
    "test": "eslint . --ext .ts && tsc --noEmit"
  },
  "dependencies": {
    "db-migrate": "^0.11.12",
    "db-migrate-mysql": "^2.1.2",
    "dotenv": "^10.0.0",
    "eris": "github:abalabahaha/eris#dev",
    "js-levenshtein": "^1.1.6",
    "mysql": "^2.18.1",
    "node-cron": "^3.0.0",
    "node-fetch": "^2.6.1",
    "pino": "^6.13.2",
    "pino-pretty": "^6.0.0",
    "slash-create": "^4.0.1"
  },
  "devDependencies": {
    "@types/js-levenshtein": "^1.1.0",
    "@types/mysql": "^2.15.19",
    "@types/node-cron": "^2.0.4",
    "@types/node-fetch": "^2.5.10",
    "@types/pino": "^6.3.11",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "eslint": "^7.28.0",
    "tsc-watch": "^4.5.0",
    "typescript": "^4.3.5"
  }
}
