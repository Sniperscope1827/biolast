{
  "name": "escape-from-discord",
  "version": "1.0.0",
  "description": "A Discord bot",
  "main": "./build/index.js",
  "author": "blobfysh",
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc-watch --noClear -p ./tsconfig.json --onSuccess \"node -r dotenv/config ./build/index.js\"",
    "start": "node -r dotenv/config build/index.js",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "migrate:up": "db-migrate up",
    "migrate:down": "db-migrate down",
    "test": "jest"
  },
  "dependencies": {
    "db-migrate": "^0.11.13",
    "db-migrate-mysql": "^2.2.0",
    "dotenv": "^10.0.0",
    "eris": "github:abalabahaha/eris#dev",
    "js-levenshtein": "^1.1.6",
    "mysql": "^2.18.1",
    "node-cron": "^3.0.0",
    "node-fetch": "^3.1.0",
    "pino": "^7.5.1",
    "pino-pretty": "^7.2.0",
    "slash-create": "^4.3.1"
  },
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "@types/js-levenshtein": "^1.1.0",
    "@types/mysql": "^2.15.19",
    "@types/node-cron": "^3.0.0",
    "@types/node-fetch": "^3.0.3",
    "@types/pino": "^7.0.5",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint": "^7.28.0",
    "jest": "^27.3.1",
    "ts-jest": "^27.0.7",
    "tsc-watch": "^4.5.0",
    "typescript": "^4.3.5"
  }
}
